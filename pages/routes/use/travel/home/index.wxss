.travel-card {
  display: flex;
  flex-direction: column;
  margin: 22rpx 40rpx 0;
  height: auto;
  background: #ffffff;
  border-radius: 10rpx;
  justify-content: space-around;
}
.travel-card image {
  width: 100%;
  height: 500rpx;
  box-shadow: 0px 0px 20px 0px rgba(0, 0, 0, 0.15);
  border-radius: 16rpx;
}
.travel-card .travelName {
  font-size: 28rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #000000;
  margin-top: 30rpx;
}
.travel-card .travelPrice {
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 24rpx;
  color: #999999;
  justify-content: space-between;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}
